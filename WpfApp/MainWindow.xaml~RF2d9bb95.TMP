<Window x:Name="mainWindow" x:Class="WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="671" Width="440" FontSize="14" ResizeMode="NoResize">
    <Grid Margin="0,0,10,0" Width="430" Height="530">
        <ListBox x:Name="destinationsListBox" Margin="10,10,225,10" FontSize="12"/>
        <CheckBox x:Name="cbCookies" Content="Cookies from _browser" HorizontalAlignment="Left" Margin="210,10,0,0" VerticalAlignment="Top" Width="172"/>
        <Button x:Name="proceedButton" Content="_Proceed" HorizontalAlignment="Left" Margin="210,452,0,0" VerticalAlignment="Top" Width="210" IsDefault="True" FontWeight="Bold"/>
        <Button Content="E_xit" HorizontalAlignment="Left" Margin="210,480,0,0" VerticalAlignment="Top" Width="210" IsCancel="True"/>
        <CheckBox x:Name="cbCustomRanges" Content="Custom _ranges" HorizontalAlignment="Left" Margin="210,33,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="listboxRanges" Margin="210,56,23,352"/>
        <Button x:Name="buttonAddRange" Content="➕" HorizontalAlignment="Left" Margin="214,250,0,0" VerticalAlignment="Top" Width="95"/>
        <Button x:Name="buttonRemoveRange" Content="➖" HorizontalAlignment="Left" Margin="315,250,0,0" VerticalAlignment="Top" Width="95"/>
        <TextBox x:Name="hourStart" HorizontalAlignment="Left" Margin="275,188,0,0" TextWrapping="Wrap" Text="00" VerticalAlignment="Top" Width="40" FontFamily="Global Monospace" TextAlignment="Center" Height="26"/>
        <TextBox x:Name="minuteStart" HorizontalAlignment="Left" Margin="322,188,0,0" TextWrapping="Wrap" Text="00" VerticalAlignment="Top" Width="40" FontFamily="Global Monospace" TextAlignment="Center" Height="26"/>
        <TextBox x:Name="secondStart" HorizontalAlignment="Left" Margin="367,188,0,0" TextWrapping="Wrap" Text="00" VerticalAlignment="Top" Width="40" FontFamily="Global Monospace" TextAlignment="Center" Height="26"/>
        <TextBox x:Name="hourEnd" HorizontalAlignment="Left" Margin="275,219,0,0" TextWrapping="Wrap" Text="00" VerticalAlignment="Top" Width="40" FontFamily="Global Monospace" TextAlignment="Center" Height="26"/>
        <TextBox x:Name="minuteEnd" HorizontalAlignment="Left" Margin="322,219,0,0" TextWrapping="Wrap" Text="00" VerticalAlignment="Top" Width="40" FontFamily="Global Monospace" TextAlignment="Center" Height="26"/>
        <TextBox x:Name="secondEnd" HorizontalAlignment="Left" Margin="367,219,0,0" TextWrapping="Wrap" Text="00" VerticalAlignment="Top" Width="40" FontFamily="Global Monospace" TextAlignment="Center" Height="26"/>
        <Label x:Name="labelStart" Content="Start" HorizontalAlignment="Left" Margin="215,188,0,0" VerticalAlignment="Top" Height="26" Width="60"/>
        <Label x:Name="labelEnd" Content="End" HorizontalAlignment="Left" Margin="215,219,0,0" VerticalAlignment="Top" Height="26" Width="60"/>

    </Grid>



</Window>

